entry class test2:

    public field j int;
    function main() returns int:
        var i = 5;
        var j = 0;
        while(i > -1) begin
            i--;
            j = 0;
            if(i == 2)
                continue;
            while(j < 7) begin
                if(j == 5)
                    break;
                print(i);
                print(j);
                print("*");
                j++;
            end
        end
        return 0;
    end
end

